# Git LFS tracking for large data files
*.parquet filter=lfs diff=lfs merge=lfs -text
data/global/* filter=lfs diff=lfs merge=lfs -text
data/**/*.parquet filter=lfs diff=lfs merge=lfs -text

# Exclude NZ files from LFS for GitHub Pages compatibility
data/nz_places_optimized.geojson !filter !diff !merge text
data/sa2.geojson !filter !diff !merge text